curl https://get.acme.sh | sh

KUMA_MODE=remote \
KUMA_MULTICLUSTER_REMOTE_ZONE=defn \
KUMA_MULTICLUSTER_REMOTE_GLOBAL_ADDRESS=grpcs://10.88.88.88:5685 kuma-cp run

cat defn-ingress.yaml | kumactl apply -f -

kumactl generate dataplane-token --dataplane=defn > defn-ingress-token

kuma-dp run --name=defn --cp-address=http://localhost:5681 --dataplane-token-file=defn-ingress-token --log-level=debug

cat defn-zone.yaml | kumactl apply -f -
